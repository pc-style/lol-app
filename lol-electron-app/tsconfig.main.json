{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/main",
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "jsx": "preserve",
    "rootDir": "./src/main"
  },
  "include": [
    "src/main/**/*"
  ],
  "exclude": [
    "src/renderer"
  ]
}